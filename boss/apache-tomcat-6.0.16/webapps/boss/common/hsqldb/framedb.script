CREATE MEMORY TABLE MONITOR_CONF(TASKID VARCHAR(30) NOT NULL,SUBTYPE VARCHAR(30) NOT NULL,NAME VARCHAR(50) NOT NULL,SOURCE CHAR(1),DESCRIPTION VARCHAR(100),OPTIONS VARCHAR(2000),SNMP CHAR(2),SNMPTARGETAGENTS VARCHAR(255))
