CREATE MEMORY TABLE SYSAPP_PROPS(APPID VARCHAR(20) NOT NULL,PROPNAME VARCHAR(30) NOT NULL,PROPVALUE VARCHAR(200))
CREATE MEMORY TABLE ECR_PLUGIN_INFO(ID VARCHAR(100) NOT NULL,IP VARCHAR(20) NOT NULL,PORT INTEGER NOT NULL,CONTEXT_PATH VARCHAR(100),PLUGIN_TYPE INTEGER NOT NULL,DEPEND_PLUGINS VARCHAR(200),ROOT_FUNC VARCHAR(30) NULL,NAME VARCHAR(30) NULL,DESCRIPTION VARCHAR(255),CREATE_DATE DATE,MODIFY_DATE DATE,PLUGIN_CLASS VARCHAR(100),PRIORITY INTEGER)
CREATE MEMORY TABLE ECR_PLUGIN_HIBERNATE(ID INTEGER NOT NULL,PLUGIN_ID VARCHAR(100) NOT NULL,CLASS_NAME VARCHAR(100) NOT NULL)
CREATE MEMORY TABLE ECR_PLUGIN_RIGHTSET(FUNC_CODE VARCHAR(30) NOT NULL,FUNC_NAME VARCHAR(80) NOT NULL,SRC_FILE VARCHAR(200),DESC_INFO VARCHAR(200),PARENT_CODE VARCHAR(30),ALIAS VARCHAR(40),SEQ_NO INTEGER,PLUGIN_ID VARCHAR(100))
CREATE MEMORY TABLE ECR_PLUGIN_VER(ID INTEGER NOT NULL,PLUGIN_ID VARCHAR(100) NOT NULL,VERSION VARCHAR(100) NOT NULL,DESCRIPTION VARCHAR(255),PUBLISH_DATE VARCHAR(50),SEQ_NO INTEGER)
