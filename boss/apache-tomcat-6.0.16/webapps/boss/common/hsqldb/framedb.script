CREATE MEMORY TABLE ECR_SERVICE_ROLE_REC(SERVICE_ROLE_CODE VARCHAR(20) NOT NULL,SERVICE_ROLE_NAME VARCHAR(40) NOT NULL,PARENT_CODE VARCHAR(30) NOT NULL,STATUS INTEGER NOT NULL,LEAFAGE_FLAG INTEGER NOT NULL)
