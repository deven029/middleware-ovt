CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE ECR_OPERATOR_REC(LOGIN_NO VARCHAR(10) NOT NULL,LOGIN_NAME VARCHAR(40),PASS_WORD VARCHAR(20) NOT NULL,PERSONNEL_CODE VARCHAR(20) NOT NULL,EFFECT_BEGIN_TIME DATE NOT NULL,EFFECT_END_TIME DATE NOT NULL,CAN_LOGIN_END_TIME CHAR(8) NOT NULL,CAN_LOGIN_BEGIN_TIME CHAR(8) NOT NULL,LOGIN_IP VARCHAR(20) NOT NULL,LOGIN_FLAG INTEGER NOT NULL,STATUS INTEGER NOT NULL,OPERATOR_TYPE INTEGER NOT NULL,PHONE VARCHAR(13),EMAIL VARCHAR(50),PERSONNEL_NAME VARCHAR(40),CITY VARCHAR(20))
