<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ovt.idtv.boss.web.db.BossCustomStat" table="BOSS_CUSTOM_STAT">
		<id name="id" type="java.lang.Long">
			<column name="ID" precision="10" scale="0" />
			<generator class="sequence">
				<param name="sequence">SEQ_BOSS_CUSTOM_STAT</param>
			</generator>
		</id>
		<property name="statName" type="java.lang.String">
			<column name="STAT_NAME"/>
		</property>
		<property name="sqlStr1" type="java.lang.String">
			<column name="SQLSTR1"/>
		</property>
		<property name="sqlStr2" type="java.lang.String">
			<column name="SQLSTR2"/>
		</property>
		<property name="sqlDesc" type="java.lang.String">
			<column name="SQLDESC"/>
		</property>
		<property name="createDate" type="java.util.Date">
			<column name="CREATE_DATE"/>
		</property>
		<property name="modifyDate" type="java.util.Date">
			<column name="MODIFY_DATE"/>
		</property>
		<property name="operNo" type="java.lang.String">
			<column name="OPER_NO"/>
		</property>
	</class>
</hibernate-mapping>
