<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ovt.idtv.boss.web.db.BossWorkOrderQueue" table="BOSS_WORKORDER_QUEUE">
		<id name="id" type="java.lang.Long">
			<column name="ID" precision="10" scale="0"/>
			<generator class="sequence">
				<param name="sequence">SEQ_BOSS_WORKORDER_QUEUE</param>
			</generator>
		</id>
		<property name="queueCode" type="java.lang.String">
			<column name="QUEUE_CODE" />
		</property>
		<property name="name" type="java.lang.String">
			<column name="NAME" length="128" />
		</property>
		<property name="queueDesc" type="java.lang.String">
			<column name="QUEUE_DESC" length="512" />
		</property>
		<property name="modifyDate" type="java.lang.String">
			<column name="MODIFY_DATE" length="14" />
		</property>
		<property name="modifyOper" type="java.lang.String">
			<column name="MODIFY_OPER" length="16" />
		</property>
		<property name="createDate" type="java.lang.String">
			<column name="CREATE_DATE" length="14" />
		</property>
		<property name="createOper" type="java.lang.String">
			<column name="CREATE_OPER" length="16" />
		</property>
	</class>
</hibernate-mapping>
