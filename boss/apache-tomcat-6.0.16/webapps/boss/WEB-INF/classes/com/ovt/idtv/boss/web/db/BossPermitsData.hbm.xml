<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ovt.idtv.boss.web.db.BossPermitsData" table="BOSS_PERMITS_DATA" >
         <id name="id" type="java.lang.Integer">
            <column name="ID" precision="10" scale="0" />
 			<generator class="sequence">  
    			 <param name="sequence">SEQ_BOSS_PERMITS_DATA</param> 
   			</generator> 
  		 </id>
        <property name="permitsId" type="java.lang.Integer">
            <column name="PERMITS_ID"/>
        </property>
        <property name="relaType" type="java.lang.Integer">
            <column name="RELA_TYPE"/>
        </property>
        <property name="dataValue" type="java.lang.String">
            <column name="DATA_VALUE" />
        </property>
        <property name="dataDesc" type="java.lang.String">
            <column name="DATA_DESC"/>
        </property>
    </class>
</hibernate-mapping>
