<a:Text>CREATE OR REPLACE TRIGGER TRI_BOSS_PERMITS_DATA
AFTER DELETE OR INSERT OR UPDATE
ON BOSS_PERMITS_DATA
REFERENCING NEW AS New OLD AS Old
FOR EACH ROW
DECLARE

BEGIN
    if deleting then
      insert into ovt_updaterecord (id,tablename,idfieldname,idvalue,operation) values (SEQ_OVT_UPDATERECORD_ID.NextVAl,&#39;BOSS_PERMITS_DATA&#39;,&#39;ID&#39;,:Old.ID,&#39;delete&#39;);
    elsif inserting then
      insert into ovt_updaterecord (id,tablename,idfieldname,idvalue,operation) values (SEQ_OVT_UPDATERECORD_ID.NextVAl,&#39;BOSS_PERMITS_DATA&#39;,&#39;ID&#39;,:New.ID,&#39;insert&#39;);

    elsif updating then
      insert into ovt_updaterecord (id,tablename,idfieldname,idvalue,operation) values (SEQ_OVT_UPDATERECORD_ID.NextVAl,&#39;BOSS_PERMITS_DATA&#39;,&#39;ID&#39;,:New.ID,&#39;update&#39;);
      end if;
END;
