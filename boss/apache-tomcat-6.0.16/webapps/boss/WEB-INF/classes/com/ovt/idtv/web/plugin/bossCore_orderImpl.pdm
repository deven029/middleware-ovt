<o:View Id="o16">
<a:ObjectID>782CFFA1-56F4-42C6-BCAF-65EBA55376FB</a:ObjectID>
<a:Name>V_ORDER_USEREQU</a:Name>
<a:Code>V_ORDER_USEREQU</a:Code>
<a:CreationDate>1302412896</a:CreationDate>
<a:Creator>longwen</a:Creator>
<a:ModificationDate>1359699294</a:ModificationDate>
<a:Modifier>jianghg</a:Modifier>
<a:View.SQLQuery> SELECT o.id AS o_id,
            o.user_id AS o_user_id,
            o.equ_id AS o_equ_id,
            o.order_type AS o_order_type,
            o.order_target_id AS o_order_target_id,
            O.FEE_TYPE_ID AS o_fee_type_id,
            O.ORDER_TIME AS o_order_time,
            O.STATUS AS o_status,
            O.PRIORITY AS o_priority,
            O.ORDER_FEE_DATA AS o_order_fee_data,
            O.IS_REORDER AS o_is_reorder,
            O.EFFECTIVE_DATE AS o_effective_date,
            O.EXPIRY_DATE AS o_expiry_date,
            O.CHARGE_DEADLINE AS o_charge_deadline,
            O.NEXT_CHARGE_MONTH AS o_next_charge_month,
            O.CHARGE_TIMES AS o_charge_times,
            O.CURR_LOCK_MONEY AS o_curr_lock_money,
            O.STOP_POSTPHONE AS o_stop_postphone,
            O.LOGIN_NO AS o_login_no,
            o.area_level1 AS o_area_level1,
            o.area_level1 AS o_area_level2,
            o.area_level1 AS o_area_level3, 
            O.DISCOUNT_TYPE AS o_discount_type,
		    O.FIELD1 AS o_field1,  
            O.CASH_PAY AS o_cash_pay,
            O.COUPON_PAY AS o_coupon_pay,
            O.PRESENT_PAY AS o_present_pay,            /*订购信息*/
            UE.EQU_STATUS AS ue_equ_status,
            UE.EQU_SLAVE_TYPE AS ue_equ_slave_type,
            UE.LABEL AS ue_label ,  
            UE.Equ_No as ue_equ_no                              /*用户与设备的关系*/
       FROM boss_user_order o, boss_user_equ ue
      WHERE     O.EQU_ID = UE.EQU_ID
            AND O.USER_ID = UE.USER_ID
   ORDER BY O.USER_ID, UE.LABEL;</a:View.SQLQuery>
<a:TaggedSQLQuery>select
   o.[{0BB1C9C1-047F-4FBF-829C-E447EEC8C94A}=id] as o_id,
