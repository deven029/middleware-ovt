<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ovt.idtv.boss.web.db.BossInvoiceReceipt" table="BOSS_INVOICE_RECEIPT"  >
        <id name="id" type="java.lang.Long">
            <column name="ID" precision="10" scale="0" />
 			<generator class="sequence">  
    			 <param name="sequence">SEQ_BOSS_INVOICE_RECEIPT</param> 
   			</generator> 
  		 </id>
  		 <property name="invoiceId" type="java.lang.Long">
            <column name="INVOICE_ID"/>
         </property>
         <property name="operReceiptId" type="java.lang.Long">
            <column name="OPER_RECEIPT_ID" />
         </property>
    </class>
</hibernate-mapping>
