    <s:attribute name="Key" type="s:string"/>
    <s:attribute name="Value" type="s:string"/>
  </s:complexType>
  <s:element name="Group" type="Group"/>
	<s:complexType name="Group" mixed="false">
		<s:complexContent mixed="false">
			<s:extension base="tns:Principal">
				<s:sequence>
					<s:element name="Type" type="s:string" minOccurs="0"/>
					<s:element name="Principals" type="tns:ArrayOfChoice1" nillable="true"/>
				</s:sequence>
			</s:extension>
		</s:complexContent>
	</s:complexType>
	<s:element name="SubscriberGroup" type="SubscriberGroup"/>
	<s:complexType name="SubscriberGroup" mixed="false">
		<s:complexContent mixed="false">
			<s:extension base="tns:Principal">
				<s:attribute name="GroupId" type="s1:guid" use="required"/>
				<s:attribute name="Name" type="s:string"/>
				<s:attribute name="Type" type="s:string"/>
			</s:extension>
		</s:complexContent>
	</s:complexType>
	<s:element name="Principal" type="Principal"/>
	<s:complexType name="Principal" abstract="true">
		<s:attribute name="ExternalID" type="s:string"/>
		<s:attribute name="Status" type="tns:PrincipalStatus" use="required"/>
	</s:complexType>
	<s:element name="User" type="User"/>
	<s:complexType name="User" mixed="false">
		<s:complexContent mixed="false">
			<s:extension base="tns:Principal">
				<s:attribute name="Pin" type="s:string"/>
				<s:attribute name="AccountExternalId" type="s:string"/>
			</s:extension>
		</s:complexContent>
	</s:complexType>
	<s:element name="PrincipalType" type="PrincipalType"/>
	<s:simpleType name="PrincipalType">
		<s:restriction base="s:string">
			<s:enumeration value="Account"/>
			<s:enumeration value="Device"/>
			<s:enumeration value="Group"/>
			<s:enumeration value="User"/>
		</s:restriction>
	</s:simpleType>
	<s:element name="LicenseRightType" type="LicenseRightType"/>
	<s:simpleType name="LicenseRightType">
		<s:restriction base="s:string">
			<s:enumeration value="Debar"/>
			<s:enumeration value="Play"/>
			<s:enumeration value="Pause"/>
			<s:enumeration value="Record"/>
			<s:enumeration value="Purchase"/>
      <s:enumeration value="Browse"/>
      <s:enumeration value="Download"/>
      <s:enumeration value="PlayLocal"/>
		</s:restriction>
	</s:simpleType>
	<s:element name="ResourceType" type="ResourceType"/>
	<s:simpleType name="ResourceType">
		<s:restriction base="s:string">
			<s:enumeration value="Package"/>
			<s:enumeration value="Service"/>
		</s:restriction>
	</s:simpleType>
	<s:element name="Condition" type="Condition"/>
	<s:complexType name="Condition" abstract="true">
		<s:attribute name="Start" type="s:dateTime" use="required"/>
		<s:attribute name="End" type="s:dateTime" use="required"/>
		<s:attribute name="Type" type="tns:ConditionType" use="required"/>
	</s:complexType>
	<s:element name="ConditionType" type="ConditionType"/>
	<s:simpleType name="ConditionType">
		<s:restriction base="s:string">
			<s:enumeration value="PlayCount"/>
			<s:enumeration value="TimeDuration"/>
			<s:enumeration value="TimeExpiration"/>
		</s:restriction>
	</s:simpleType>
	<s:element name="Offer" type="Offer"/>
	<s:complexType name="Offer">
		<s:sequence>
			<s:element name="Conditions" type="tns:ArrayOfConditions" nillable="true"/>
		</s:sequence>
		<s:attribute name="ExternalId" type="s:string"/>
		<s:attribute name="OfferId" type="s1:guid" use="required"/>
		<s:attribute name="ResourceId" type="s1:guid" use="required"/>
		<s:attribute name="ResourceExternalId" type="s:string"/>
		<s:attribute name="PrincipalExternalId" type="s:string"/>
		<s:attribute name="PrincipalType" type="tns:PrincipalType" use="required"/>
		<s:attribute name="Price" type="s:string"/>
		<s:attribute name="Currency" type="s:string"/>
		<s:attribute name="Tax" type="s:string"/>
		<s:attribute name="Message" type="s:string"/>
		<s:attribute name="Recurrent" type="s:boolean" use="required"/>
		<s:attribute name="StartDate" type="s:dateTime" use="required"/>
		<s:attribute name="Expiration" type="s:dateTime" use="required"/>
		<s:attribute name="SubSystem" type="s:string"/>
	</s:complexType>
	<!--<s:element name="ArrayOfString" type="ArrayOfString"/>-->
	<s:complexType name="ArrayOfString">
		<s:sequence>
			<s:element name="string" type="s:string" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
		</s:sequence>
	</s:complexType>
	<!--<s:element name="ArrayOfOffer" type="ArrayOfOffer"/>-->
	<s:complexType name="ArrayOfOffer">
		<s:sequence>
			<s:element name="Offer" type="tns:Offer" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
		</s:sequence>
	</s:complexType>
	<!--<s:element name="ArrayOfConditions" type="ArrayOfConditions"/>-->
	<s:complexType name="ArrayOfConditions">
		<s:choice minOccurs="0" maxOccurs="unbounded">
			<s:element name="TimeDuration" type="tns:TimeDuration" nillable="true"/>
			<s:element name="TimeExpiration" type="tns:TimeExpiration" nillable="true"/>
			<s:element name="PlayCount" type="tns:PlayCount" nillable="true"/>
		</s:choice>
	</s:complexType>
	<s:element name="PlayCount" type="PlayCount"/>
	<s:complexType name="PlayCount" mixed="false">
		<s:complexContent mixed="false">
			<s:extension base="tns:Condition">
				<s:attribute name="Count" type="s:unsignedInt" use="required"/>
			</s:extension>
		</s:complexContent>
	</s:complexType>
	<s:element name="TimeDuration" type="TimeDuration"/>
	<s:complexType name="TimeDuration" mixed="false">
		<s:complexContent mixed="false">
			<s:extension base="tns:Condition">
				<s:attribute name="DurationInSecs" type="s:long" use="required"/>
			</s:extension>
		</s:complexContent>
	</s:complexType>
	<s:element name="TimeExpiration" type="TimeExpiration"/>
	<s:complexType name="TimeExpiration" mixed="false">
		<s:complexContent mixed="false">
			<s:extension base="tns:Condition">
				<s:attribute name="Expiration" type="s:dateTime" use="required"/>
			</s:extension>
		</s:complexContent>
	</s:complexType>
	<s:element name="Grant" type="Grant"/>
	<s:complexType name="Grant">
		<s:sequence>
			<s:element name="Conditions" type="tns:ArrayOfConditions" nillable="true"/>
		</s:sequence>
		<s:attribute name="ExternalId" type="s:string"/>
		<s:attribute name="ResourceId" type="s1:guid" use="required"/>
		<s:attribute name="ResourceExternalId" type="s:string"/>
		<s:attribute name="PrincipalExternalId" type="s:string"/>
		<s:attribute name="PrincipalType" type="tns:PrincipalType" use="required"/>
		<s:attribute name="ResType" type="tns:ResourceType" use="required"/>
		<s:attribute name="Right" type="tns:LicenseRightType" use="required"/>
	</s:complexType>
	<!--<s:element name="ArrayOfGrant" type="ArrayOfGrant"/>-->
	<s:complexType name="ArrayOfGrant">
		<s:sequence>
			<s:element name="Grant" type="tns:Grant" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
		</s:sequence>
	</s:complexType>
	<s:element name="GrantStatus" type="GrantStatus"/>
	<s:simpleType name="GrantStatus">
		<s:restriction base="s:string">
			<s:enumeration value="CreditLimitExceeded"/>
			<s:enumeration value="Error"/>
			<s:enumeration value="Success"/>
		</s:restriction>
	</s:simpleType>
	<s:simpleType name="BillingEventStatus">
		<s:restriction base="s:string">
			<s:enumeration value="UnRead"/>
			<s:enumeration value="Read"/>
			<s:enumeration value="Recorded"/>
			<s:enumeration value="Deletable"/>
		</s:restriction>
	</s:simpleType>
	<s:element name="BillingEventStatus" type="BillingEventStatus"/>
	<s:element name="PinStatus" type="PinStatus"/>
	<s:simpleType name="PinStatus">
		<s:restriction base="s:string">
			<s:enumeration value="NoError"/>
			<s:enumeration value="Forbidden"/>
			<s:enumeration value="NoMatch"/>
		</s:restriction>
	</s:simpleType>
	<s:element name="PinInfo" type="PinInfo"/>
	<s:complexType name="PinInfo">
		<s:attribute name="Pin" type="s:string"/>
		<s:attribute name="PinType" type="s:short" use="required"/>
		<s:attribute name="EnableDate" type="s:dateTime" use="required"/>
	</s:complexType>
	<s:element name="BillingRecord" type="BillingRecord"/>
	<s:complexType name="BillingRecord">
		<s:attribute name="PrincipalExternalId" type="s:string"/>
		<s:attribute name="PrincipalType" type="tns:PrincipalType" use="required"/>
		<s:attribute name="Id" type="s1:guid" use="required"/>
		<s:attribute name="ResourceId" type="s1:guid" use="required"/>
		<s:attribute name="ResourceExternalId" type="s:string"/>
		<s:attribute name="PurchaseDateTime" type="s:dateTime" use="required"/>
		<s:attribute name="Price" type="s:string"/>
		<s:attribute name="ExternalId" type="s:string"/>
		<s:attribute name="Status" type="tns:BillingEventStatus" use="required"/>
		<s:attribute name="SubSystem" type="s:string"/>
		<s:attribute name="StartDate" type="s:dateTime" use="required"/>
		<s:attribute name="EndDate" type="s:dateTime" use="required"/>
	</s:complexType>
	<!-- Package types-->
