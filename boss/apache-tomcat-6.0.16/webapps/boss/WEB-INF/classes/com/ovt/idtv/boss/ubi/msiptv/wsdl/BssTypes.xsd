	<s:complexType name="ArrayOfPackage">
		<s:sequence>
			<s:element name="Package" type="tns:Package" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
		</s:sequence>
	</s:complexType>
	<s:element name="Package" type="Package"/>
	<s:complexType name="Package">
		<s:sequence>
			<s:element name="ServiceIds" type="tns:ArrayOfGuid" nillable="true"/>
			<s:element name="Packages" type="tns:ArrayOfGuid" nillable="true"/>
		</s:sequence>
		<s:attribute name="Name" type="s:string"/>
		<s:attribute name="PackageId" type="s1:guid" use="required"/>
	</s:complexType>
	<s:element name="PackageOffer" type="PackageOffer"/>
	<s:complexType name="PackageOffer">
		<s:sequence>
			<s:element name="Price" type="s:double"/>
