	<s:element name="Device" type="Device"/>
	<s:complexType name="Device" mixed="false">
		<s:complexContent mixed="false">
			<s:extension base="tns:Principal">
				<s:attribute name="AccountExternalId" type="s:string"/>
				<s:attribute name="Pin" type="s:string"/>
				<s:attribute name="ID" type="s1:guid" use="required"/>
				<s:attribute name="Version" type="s:string"/>
				<s:attribute name="RequestedVersion" type="s:string"/>
				<s:attribute name="ResolvedVersion" type="s:string"/>
				<s:attribute name="LastReportedVersion" type="s:string"/>
			</s:extension>
		</s:complexContent>
	</s:complexType>
	<s:element name="DeviceValue" type="DeviceValue"/>
	<s:complexType name="DeviceValue">
		<s:attribute name="Key" type="s:string"/>
		<s:attribute name="Value" type="s:string"/>
	</s:complexType>
  <s:element name="AccountValue" type="AccountValue"/>
