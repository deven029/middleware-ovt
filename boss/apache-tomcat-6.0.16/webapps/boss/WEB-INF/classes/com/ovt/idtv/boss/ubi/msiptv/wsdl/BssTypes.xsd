	<s:element name="Account" type="Account"/>
	<s:complexType name="Account" mixed="false">
		<s:complexContent mixed="false">
			<s:extension base="tns:Principal">
				<s:sequence>
					<s:element name="Devices" type="tns:ArrayOfDevice" nillable="true"/>
					<s:element name="Users" type="tns:ArrayOfUser" nillable="true"/>
				</s:sequence>
				<s:attribute name="CreditLimit" type="s:double" use="required"/>
				<s:attribute name="TimeZone" type="tns:TimeZonesEnumeration" use="required"/>
				<s:attribute name="HDStreams" type="s:int" use="optional"/>
				<s:attribute name="SDStreams" type="s:int" use="optional"/>
        <s:attribute name="DVREnabled" type="s:boolean" use="optional"/>
        <s:attribute name="RecordingDeviceId" type="s1:guid" use="optional"/>
        <s:attribute name="Pin" type="s:string"/>
				<s:attribute name="ServiceGroupName" type="s:string" use="optional"/>
			</s:extension>
		</s:complexContent>
	</s:complexType>
	<!--<s:element name="ArrayOfAccount" type="ArrayOfAccount"/>-->
	<s:complexType name="ArrayOfAccount">
		<s:sequence>
