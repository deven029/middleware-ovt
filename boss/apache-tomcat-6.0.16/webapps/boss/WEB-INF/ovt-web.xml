<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<filters>
		<filter>
			<filter-name>webFramePagesFilter</filter-name>
			<filter-class>com.ovt.common.web.filter.WebFramePagesFilter
			</filter-class>
		</filter>
	</filters>
	<filter-mappings>
		<filter-mapping>
			<filter-name>webFramePagesFilter</filter-name>
			<url-pattern>/common/*</url-pattern>
			<dispatcher>FORWARD</dispatcher>
	  		<dispatcher>REQUEST</dispatcher>
		</filter-mapping>
	</filter-mappings>
	<servlets>
		<servlet>
			<servlet-name>DisplayChart</servlet-name>
			<servlet-class>
				org.jfree.chart.servlet.DisplayChart
		</servlet-class>
		</servlet>
	</servlets>
	<servlet-mappings>
		<servlet-mapping>
			<servlet-name>DisplayChart</servlet-name>
			<url-pattern>/DisplayChart</url-pattern>
		</servlet-mapping>
	</servlet-mappings>
</web-app>
